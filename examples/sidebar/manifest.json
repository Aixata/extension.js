{
  "manifest_version": 3,
  "version": "0.0.1",
  "name": "sidebar_action",
  "author": "Cezar Augusto",
  "description": "An Extension.js example.",
  "chromium:side_panel": {
    "default_path": "sidebar/index.html",
    "default_title": "Sidebar Action",
